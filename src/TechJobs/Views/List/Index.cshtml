<h1>@ViewBag.Title</h1>
<h2>View All</h2>
<ul>
    @foreach (KeyValuePair<string, string> column in ViewBag.columns) {                 //generate links to ...list/Values?column="column.key"
        <li>
            <a asp-controller="List" asp-action="Values" asp-route-column="@column.Key">@column.Value</a>   
        </li>
    }
</ul>